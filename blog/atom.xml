---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>Andrew Yuwono's Blog</title>
  <link href="http://ayuw.me/blog/atom.xml" rel="self"/>
  <link href="http://ayuw.me/blog"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://ayuw.me/blog</id>
  <author>
    <name>Andrew Yuwono</name>
    <email></email>
  </author>
  
  {% for post in site.posts %}
    <entry>
			<title>{{ post.title }}</title>
			<link href="http://ayuw.me{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://ayuw.me/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
